{
  "name": "Unsplash view image",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "http://localhost",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://unsplash.com/*"],
      "js": ["src/unsplash.js"]
    }
  ],
  "permissions": [
    "https://images.unsplash.com/*"
  ]
}